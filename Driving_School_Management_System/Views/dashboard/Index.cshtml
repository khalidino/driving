@{
    ViewBag.Title = "Dashboard";
}

 

<div class="row">
    <div class="col-md-6">
        <!-- small box -->
        <div class="small-box bg-purple">
            <div class="inner">


                <h3 class="text-center"> REPORTS</h3>
            </div>
            <div class="icon">
                <i class="ion ion-bag"></i>
            </div>
            <a href="/reports/search" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
        </div>
    </div>
    <!-- ./col -->
    <div class="col-md-6">
        <!-- small box -->
        <div class="small-box bg-secondary">
            <div class="inner">


                <h3 class="text-center"> USER MANAGEMENT</h3>
            </div>
            <div class="icon">
                <i class="ion ion-stats-bars"></i>
            </div>
            <a href="/userdashboard" class="small-box-footer">Visit User <i class="fas fa-arrow-circle-right"></i></a>
        </div>
    </div>


</div>





<div class="row">
    <div class="col-md-3">
        <!-- small box -->
        <div class="small-box bg-info">
            <div class="inner">
                <h3 class="text-center ">@ViewBag.Durations</h3>

                <h3 class="text-center  text-uppercase">Durations</h3>
            </div>
            <div class="icon">
                <i class="ion ion-bag"></i>
            </div>
         </div>
    </div>
    <!-- ./col -->
    <div class="col-md-3">
        <!-- small box -->
        <div class="small-box bg-success">
            <div class="inner">
                <h3 class="text-center">@ViewBag.Shifts</h3>

                <h3 class="text-center  text-uppercase">Shifts</h3>
            </div>
            <div class="icon">
                <i class="ion ion-stats-bars"></i>
            </div>
         </div>
    </div>
    <!-- ./col -->
    <div class="col-md-3">
        <!-- small box -->
        <div class="small-box bg-warning">
            <div class="inner">
                <h3 class="text-center">@ViewBag.Students</h3>

                <h3 class="text-center  text-uppercase">Students</h3>
            </div>
            <div class="icon">
                <i class="ion ion-person-add"></i>
            </div>
         </div>
    </div>
    <!-- ./col -->
    <div class="col-md-3">
        <!-- small box -->
        <div class="small-box bg-danger">
            <div class="inner">
                <h3 class="text-center">@ViewBag.Attendence</h3>

                <h3 class="text-center text-uppercase">Attendence</h3>
            </div>
            <div class="icon">
                <i class="ion ion-pie-graph"></i>
            </div>
         </div>
    </div>

</div>




<div class="row">
    <div class="col-md-3">
        <!-- small box -->
        <div class="small-box bg-purple">
            <div class="inner">
                <h3 class="text-center ">@ViewBag.EMPLOYEE</h3>

                <h3 class="text-center">EMPLOYEE</h3>
            </div>
            <div class="icon">
                <i class="ion ion-bag"></i>
            </div>
         </div>
    </div>
    <!-- ./col -->
    <div class="col-md-3">
        <!-- small box -->
        <div class="small-box bg-secondary">
            <div class="inner">
                <h3 class="text-center">@ViewBag.INSTRUCTOR</h3>

                <h3 class="text-center">INSTRUCTOR</h3>
            </div>
            <div class="icon">
                <i class="ion ion-stats-bars"></i>
            </div>
         </div>
    </div>
    <!-- ./col -->
    <div class="col-md-3">
        <!-- small box -->
        <div class="small-box bg-info">
            <div class="inner">
                <h3 class="text-center">@ViewBag.CAR</h3>

                <h3 class="text-center">CAR DETAIL</h3>
            </div>
            <div class="icon">
                <i class="ion ion-person-add"></i>
            </div>
         </div>
    </div>
    <!-- ./col -->
    <div class="col-md-3">
        <!-- small box -->
        <div class="small-box bg-danger">
            <div class="inner">
                <h3 class="text-center">@ViewBag.NEWS</h3>

                <h3 class="text-center">NEWS</h3>
            </div>
            <div class="icon">
                <i class="ion ion-pie-graph"></i>
            </div>
         </div>
    </div>

</div>


